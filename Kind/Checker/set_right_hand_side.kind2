// Set the @Kind.Checker@ to the right hand side mode.
Kind.Checker.set_right_hand_side (rhs: Bool) : Kind.Checker Unit
Kind.Checker.set_right_hand_side to_rhs = context => hole_count => depth => rhs => subst => eqts => errs =>
  Kind.Result.checked context hole_count depth to_rhs subst eqts errs Unit.new
